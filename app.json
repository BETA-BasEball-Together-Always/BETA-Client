{
  "expo": {
    "name": "Beta",
    "slug": "Beta",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "assetBundlePatterns": [
      "src/shared/assets/fonts/*"
    ],
    "splash": {
      "image": "./assets/splash-screen.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "야구네컷 촬영을 위해 카메라 접근이 필요합니다.",
        "NSPhotoLibraryAddUsageDescription": "편집된 야구네컷을 사진 보관함에 저장합니다.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.bmbx08.Beta"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.CAMERA"
      ],
      "package": "com.bmbx08.Beta"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "react-native-vision-camera",
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "cb0869cf-b218-49c9-a57c-e29944ee41fb"
      }
    },
    "owner": "bmbx08"
  }
}
